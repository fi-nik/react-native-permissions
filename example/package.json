{
  "name": "react-native-permissions-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "appium": "appium",
    "clean-modules": "rm -rf ./node_modules/react-native-permissions/{example,node_modules}",
    "clean": "rm -rf ./node_modules ./ios/Pods",
    "pod-update": "cd ./ios && pod update && cd ..",
    "start": "react-native start",
    "reinstall": "yarn clean && yarn install",
    "test:windows": "yarn jest --setupFiles=./jest-setups/jest.setup.windows.js",
    "start:macos": "node node_modules/react-native-macos/local-cli/cli.js start --use-react-native-macos"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "react": "16.11.0",
    "react-native": "~0.62.0",
    "react-native-macos": "^0.62.0-0",
    "react-native-paper": "4.4.1",
    "react-native-permissions": "../",
    "react-native-vector-icons": "7.1.0",
    "react-native-windows": "^0.62.0-0"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/runtime": "7.12.5",
    "@types/jest": "24.9.1",
    "@types/react": "16.14.2",
    "@types/react-native": "0.63.37",
    "@types/react-test-renderer": "16.9.4",
    "appium": "1.19.1",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.59.0",
    "patch-package": "6.2.2",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.2.1",
    "react-test-renderer": "16.11.0",
    "selenium-appium": "0.0.16",
    "selenium-webdriver": "4.0.0-alpha.7",
    "typescript": "4.1.2"
  }
}